{
  "name": "pdfelectron",
  "version": "1.1.2",
  "description": "인사계 PDF 프로그램",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "electron-builder --windows nsis:ia32",
    "build": "electron-builder --windows portable"
  },
  "keywords": [],
  "author": "안민수",
  "license": "ISC",
  "devDependencies": {
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-store": "^10.1.0"
  },
  "build": {
    "productName": "InsaPDF",
    "appId": "com.electron.hello",
    "asar": true,
    "protocols": {
      "name": "pdfelectron",
      "schemes": [
        "pdfelectron"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./img/brightness-stand.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ]
    }
  },
  "type": "module"
}
